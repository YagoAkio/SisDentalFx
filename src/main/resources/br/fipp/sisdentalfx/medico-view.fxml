<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.fipp.sisdentalfx.MedicoViewController">
   <children>
      <Label prefHeight="17.0" prefWidth="597.0" text="ID">
         <VBox.margin>
            <Insets left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="tfID">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" />
         </VBox.margin>
      </TextField>
      <Label prefHeight="17.0" prefWidth="597.0" text="Nome">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="tfNome">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" />
         </VBox.margin>
      </TextField>
      <Label prefHeight="17.0" prefWidth="597.0" text="CRO">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="tfCRO" layoutX="10.0" layoutY="69.0">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" />
         </VBox.margin>
      </TextField>
      <Label layoutX="10.0" layoutY="94.0" prefHeight="17.0" prefWidth="597.0" text="Fone">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="tfFone" layoutX="10.0" layoutY="111.0">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" />
         </VBox.margin>
      </TextField>
      <Label layoutX="10.0" layoutY="136.0" prefHeight="17.0" prefWidth="597.0" text="Email">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="tfEmail" layoutX="10.0" layoutY="153.0">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" />
         </VBox.margin>
      </TextField>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="btnAceitar" mnemonicParsing="false" prefWidth="90.0" style="-fx-background-color: lightblue;" text="Confirmar" textFill="#696cab">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnRecusar" mnemonicParsing="false" prefWidth="90.0" style="-fx-background-color: red;" text="Cancelar">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
         <opaqueInsets>
            <Insets left="10.0" right="10.0" />
         </opaqueInsets>
         <VBox.margin>
            <Insets right="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
